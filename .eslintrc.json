{
  "env": {
    "browser": true,
    "es2021": true
  },
  //  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "requireConfigFile": false,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
